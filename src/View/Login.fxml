<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <Label fx:id="headerLabel" alignment="CENTER" layoutX="175.0" layoutY="36.0" text="Login">
         <font>
            <Font name="System Bold" size="34.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="115.0" layoutY="116.0" prefHeight="34.0" prefWidth="220.0" promptText="Username">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button fx:id="loginButton" layoutX="199.0" layoutY="226.0" mnemonicParsing="false" onAction="#loginButtonClicked" text="Login" />
      <PasswordField fx:id="passwordTextField" layoutX="115.0" layoutY="164.0" prefHeight="34.0" prefWidth="220.0" promptText="Password">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Label fx:id="locationLabel" layoutX="14.0" layoutY="269.0" text="Location :" />
      <Label fx:id="userLocationLabel" layoutX="73.0" layoutY="269.0" prefHeight="17.0" prefWidth="110.0" />
   </children>
</AnchorPane>
